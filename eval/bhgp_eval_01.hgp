verify_signature ( y => a );

cell c( y => a )
{
    w = [ 2 ][ dimof( a ) ] 1;
    b = [ 2 ] 1;
    y = w ** a + b;
};

cell x( y => n, a )
{
    y = if( n > 0 ) then x( n - 1 , c( a ) );
        else                        c( a );
};

// ackermann
cell ack( y => n, m )
{
    y =
      if     ( n == 0 ) then m + 1;
      else if( m == 0 ) then ack( n - 1, 1 );
      else                   ack( n - 1, ack( n, m - 1 ) );
};

y = x( 1000 ) : ( a );

