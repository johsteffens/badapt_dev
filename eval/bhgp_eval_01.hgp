verify_signature ( y => a );

cell c( y => a )
{
    w = [ 2 ][ dimof( a ) ] 1;
    b = [ 2 ] 1;
    y = w**a+b;
};

cell x( y => n, a )
{
    y = if(n>0) then x(n - 1 , c( a ) ); else c( a );
};

