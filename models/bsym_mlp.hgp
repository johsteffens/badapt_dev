// multilayer perceptron
graph mlp( y ) => ( out_dim, x )
{
	graph layer( y ) => ( dim_y, x )
	{
        holor adaptive w = [ dim_y ][ dimof( x ) ]#;
        holor adaptive b = [ dim_y ]#;

        y -> w <*> x + b;
    };

	graph hid_layer = tanh : layer();
	graph out_layer = tanh : layer( out_dim );

	y -> out_layer : hid_layer( 50 ) : hid_layer( 100 ) : x;
};

output -> mlp( out_size ) : input;

